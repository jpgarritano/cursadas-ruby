<%= simple_form_for(@test) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :date %>
    <%= f.input :minimum_grade %>
    <%= f.association :course %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
